{
  "name": "reasonable-accommodation-generator",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@emailjs/browser": "^4.4.1",
    "@react-pdf/renderer": "^4.3.0",
    "@supabase/supabase-js": "^2.58.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "cytoscape": "^3.33.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "lodash": "^4.17.21",
    "next": "^15.0.0",
    "react": "^18.3.1",
    "react-cytoscapejs": "^2.0.0",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.8.2",
    "react-scripts": "5.0.1",
    "tailwindcss": "^3.4.18",
    "web-vitals": "^3.5.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:react": "PORT=3001 POSTCSS_CONFIG=postcss.config.react.js react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "next dev",
    "dev:hybrid": "concurrently \"npm run dev\" \"npm run start:react\"",
    "build:next": "next build",
    "export": "next export",
    "start:next": "next start",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "build:static": "npm run generate-sitemap && next build",
    "worker:dev": "wrangler dev --var ORIGIN_NEXT=http://127.0.0.1:3000 --var ORIGIN_APP=http://127.0.0.1:3001",
    "worker:deploy": "wrangler deploy",
    "dev:full": "concurrently \"npm run dev\" \"npm run start:react\" \"npm run worker:dev\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^24.6.2",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "html-webpack-plugin": "^5.6.3",
    "postcss": "^8.5.4",
    "postcss-loader": "^8.1.1",
    "typescript": "^4.9.5",
    "wrangler": "^3.0.0"
  }
}
